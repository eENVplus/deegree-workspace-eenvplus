	<QueryCRS>EPSG:3043</QueryCRS>
	<QueryCRS>EPSG:3044</QueryCRS>
	<QueryCRS>EPSG:3045</QueryCRS>
	<QueryCRS>EPSG:3046</QueryCRS>
	<QueryCRS>EPSG:3047</QueryCRS>
	<QueryCRS>EPSG:3048</QueryCRS>
	<QueryCRS>EPSG:3049</QueryCRS>
	<QueryCRS>EPSG:3050</QueryCRS>
	<QueryCRS>EPSG:3051</QueryCRS>
	<QueryCRS>EPSG:4258</QueryCRS>
