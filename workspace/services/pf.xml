<?xml version="1.0" encoding="utf-8"?>
<deegreeWFS xmlns="http://www.deegree.org/services/wfs" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" configVersion="3.2.0" xsi:schemaLocation="http://www.deegree.org/services/wfs http://schemas.deegree.org/services/wfs/3.2.0/wfs_configuration.xsd">
	<SupportedVersions>
    <Version>2.0.0</Version>
    <Version>1.1.0</Version>
	</SupportedVersions>
  
	<FeatureStoreId>inspire_pf</FeatureStoreId>
  
  <!-- WFS-T -->
  <EnableTransactions idGen="GenerateNew">false</EnableTransactions>
  
	<!-- The first CRS is the default one -->
	<QueryCRS>EPSG:3035</QueryCRS>
  
	<!-- Other CRSs -->
	<QueryCRS>EPSG:3034</QueryCRS>
	<QueryCRS>EPSG:3038</QueryCRS>
	<QueryCRS>EPSG:3039</QueryCRS>
	<QueryCRS>EPSG:3040</QueryCRS>
	<QueryCRS>EPSG:3041</QueryCRS>
